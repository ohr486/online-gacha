<p id="notice"><%= notice %></p>

<h1>Results</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Reward</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td><%= result.user_id %></td>
        <td><%= result.reward_id %></td>
        <td><%= link_to 'Show', [:admin, result] %></td>
        <td><%= link_to 'Edit', edit_admin_result_path(result) %></td>
        <td><%= link_to 'Destroy', [:admin, result], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result', new_admin_result_path %>
